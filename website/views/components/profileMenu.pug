//- button(onclick="window.location='http://localhost:8080/auth/logout';") Logout
.action 
    .profileImageWrapper(onclick="profileMenuToggle();")
        img(src=loggedInUser.avatar, alt="")
    .profileMenu 
        h3 #{`${loggedInUser.username}#${loggedInUser.discriminator}` }
            | <br>
            if (playerCharacter)
                span Tess Von Elbencort
        if (playerCharacter)
            .profileMenuCategory
                ul 
                    li
                        i.fas.fa-address-card
                        a(href="#") My Profile
                    li
                        i.fas.fa-address-card
                        a(href="#") Test 1
        .profileMenuCategory
            
            ul
                li
                    i.fas.fa-address-card
                    a(href="#") Test 2
                li
                    i.fas.fa-address-card
                    a(href="#") Test 3
        .profileMenuCategory          
            ul
                li
                    i.fas.fa-sliders-h
                    a(href="#") Preferences
                li
                    i.fas.fa-sign-out-alt
                    a(href=`${baseURL}:${port}/auth/logout`) Logout